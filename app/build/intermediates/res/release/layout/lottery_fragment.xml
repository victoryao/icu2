<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/Widget.ScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        style="@style/Widget.Item.ListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <include layout="@layout/main_tab_header_view" />
        
        <FrameLayout
            android:id="@+id/item_xianchanggou_nfc_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/lottery_photo_box_height"
            android:paddingBottom="@dimen/home_cellspacing"
            android:visibility="gone" >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom"
                android:adjustViewBounds="true"
                android:focusable="false"
                android:scaleType="fitXY"
                android:src="@drawable/xianchanggou_scan" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom"
                android:focusable="false"
                android:scaleType="fitXY"
                android:src="@drawable/lottery_item_photo_bg" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/item_xianchanggou_scan_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/lottery_photo_box_height"
            android:paddingBottom="@dimen/home_cellspacing"
            android:visibility="gone" >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom"
                android:adjustViewBounds="true"
                android:focusable="false"
                android:scaleType="fitXY"
                android:src="@drawable/xianchanggou_scan" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom"
                android:focusable="false"
                android:scaleType="fitXY"
                android:src="@drawable/lottery_item_photo_bg" />
        </FrameLayout>
        
        <LinearLayout
            style="@style/Widget.Item.ListView"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" >
            <RelativeLayout
	            android:id="@+id/xianhuo_sales"
	            style="@style/Widget.Item.ListView.SingleLine"
	            android:background="@drawable/list_item_middle_bg" >
	
	            <ImageView
	                android:id="@+id/account_nfc_find_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="15dip"
	                android:adjustViewBounds="true"
	                android:clickable="false"
	                android:focusable="false"
	                android:src="@drawable/openbox_icon" />
	
	            <TextView
	                style="@style/TextAppearance.List.Primary"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_centerVertical="true"
	                android:layout_toRightOf="@+id/account_nfc_find_icon"
	                android:text="@string/xianhuo_sales_text"
	                android:textSize="@dimen/text_size_medium" />
	
	            <ImageView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:clickable="false"
	                android:focusable="false"
	                android:scaleType="centerInside"
	                android:src="@drawable/btn_next" />
	        </RelativeLayout>
	        
	        <RelativeLayout
	            android:id="@+id/account_nfc_find"
	            style="@style/Widget.Item.ListView.SingleLine"
	            android:background="@drawable/list_item_middle_bg" >
	
	            <ImageView
	                android:id="@+id/account_nfc_find_icon2"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="15dip"
	                android:adjustViewBounds="true"
	                android:clickable="false"
	                android:focusable="false"
	                android:src="@drawable/repair_icon" />
	
	            <TextView
	                style="@style/TextAppearance.List.Primary"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_centerVertical="true"
	                android:layout_toRightOf="@+id/account_nfc_find_icon2"
	                android:text="@string/product_find_nfc_text"
	                android:textSize="@dimen/text_size_medium" />
	
	            <ImageView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:clickable="false"
	                android:focusable="false"
	                android:scaleType="centerInside"
	                android:src="@drawable/btn_next" />
	        </RelativeLayout>
        
        </LinearLayout>
        
        <RelativeLayout
            android:id="@+id/kong"
            style="@style/Widget.Item.ListView.Label">
            <TextView
                style="@style/TextAppearance.List.Primary"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_centerVertical="true" />
           
        </RelativeLayout>
      
        
        
        <RelativeLayout
            android:id="@+id/account_nfc_add"
            style="@style/Widget.Item.ListView.SingleLine"
            android:background="@drawable/list_item_top_bg" >

            <ImageView
                android:id="@+id/account_nfc_add_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dip"
                android:adjustViewBounds="true"
                android:clickable="false"
                android:focusable="false"
                android:src="@drawable/edit_icon" />

            <TextView
                style="@style/TextAppearance.List.Primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/account_nfc_add_icon"
                android:text="@string/account_nfc_add_text"
                android:textSize="@dimen/text_size_medium" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:clickable="false"
                android:focusable="false"
                android:scaleType="centerInside"
                android:src="@drawable/btn_next" />
             
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/kong2"
            style="@style/Widget.Item.ListView.Label">
            <TextView
                style="@style/TextAppearance.List.Primary"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_centerVertical="true" />
           
        </RelativeLayout>
        
         <RelativeLayout
            android:id="@+id/same_day_return"
            style="@style/Widget.Item.ListView.SingleLine"
            android:background="@drawable/list_item_middle_bg" >

            <ImageView
                android:id="@+id/same_day_return_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dip"
                android:adjustViewBounds="true"
                android:clickable="false"
                android:focusable="false"
                android:src="@drawable/return_icon" />

            <TextView
