                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:clickable="false"
                android:focusable="false"
                android:scaleType="centerInside"
                android:src="@drawable/btn_next" />
             
        </RelativeLayout>
        
         <RelativeLayout
            android:id="@+id/same_day_exchange"
            style="@style/Widget.Item.ListView.SingleLine"
            android:background="@drawable/list_item_middle_bg" >

            <ImageView
                android:id="@+id/same_day_exchange_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dip"
                android:adjustViewBounds="true"
                android:clickable="false"
                android:focusable="false"
                android:src="@drawable/exchange_icon" />

            <TextView
                style="@style/TextAppearance.List.Primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/same_day_exchange_icon"
                android:text="@string/same_day_exchange_text"
                android:textSize="@dimen/text_size_medium" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:clickable="false"
                android:focusable="false"
                android:scaleType="centerInside"
                android:src="@drawable/btn_next" />
             
        </RelativeLayout>
         
        <RelativeLayout
            android:id="@+id/account_1d_find"
            style="@style/Widget.Item.ListView.SingleLine"
            android:background="@drawable/list_item_bottom_bg" 
            android:visibility="gone">

            <ImageView
                android:id="@+id/account_1d_find_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dip"
                android:adjustViewBounds="true"
                android:clickable="false"
                android:focusable="false"
                android:src="@drawable/address_icon" />

            <TextView
                style="@style/TextAppearance.List.Primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/account_1d_find_icon"
                android:text="@string/product_find_1d_text"
                android:textSize="@dimen/text_size_medium" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
